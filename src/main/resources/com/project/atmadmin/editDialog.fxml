<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.project.atmadmin.controllers.DialogController">

    <headerText>
        Fill in the information for to update the data
    </headerText>

    <content>
        <GridPane vgap="10" hgap="10">

            <Label text="First Name: " GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="fNameField" GridPane.rowIndex="2" GridPane.columnIndex="1"/>

            <Label text="Last Name: " GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="lNameField" GridPane.rowIndex="3" GridPane.columnIndex="1"/>

            <Label text="Address: " GridPane.rowIndex="4" GridPane.columnIndex="0"/>
            <TextField fx:id="addressField" GridPane.rowIndex="4" GridPane.columnIndex="1"/>

            <Label text="Email: " GridPane.rowIndex="5" GridPane.columnIndex="0"/>
            <TextField fx:id="emailField" GridPane.rowIndex="5" GridPane.columnIndex="1"/>

            <Label text="Occupation: " GridPane.rowIndex="6" GridPane.columnIndex="0"/>
            <TextField fx:id="occupationField" GridPane.rowIndex="6" GridPane.columnIndex="1"/>

        </GridPane>
    </content>

</DialogPane>
